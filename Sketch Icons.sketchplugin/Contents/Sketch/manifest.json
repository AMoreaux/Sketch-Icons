{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "name": "Sketch Icons",
  "identifier": "sketch.icons.plugin",
  "version": "1.0.0",
  "description": "Import your icons, apply a color mask and create a dynamic icons library.",
  "authorEmail": "moreaux.antoine@gmail.com",
  "author": "Valentin Chr√©tien, Antoine Moreaux",
  "commands": [
    {
      "script": "commands.js",
      "handler": "importIcons",
      "shortcut": "command shift y",
      "name": "Import icons...",
      "identifier": "import",
      "description": "Import your icons library"
    },
    {
      "script": "commands.js",
      "handler": "updateIconsOnSelectedArtboards",
      "name": "Update icons on selected artboards...",
      "identifier": "update.selected.artboard",
      "description": "Update icons on selected artboards"
    },
    {
      "script": "commands.js",
      "handler": "addMaskOnSelectedArtboards",
      "name": "Add color mask on selected artboards...",
      "identifier": "mask.selected.artboard",
      "description": "Apply mask on selecteds artboard"
    }
  ],
  "menu": {
    "title": "Sketch Icons",
    "items": [
      "import",
      "update.selected.artboard",
      "-",
      "mask.selected.artboard"
    ]
  },
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com//master/.appcast.xml"
}